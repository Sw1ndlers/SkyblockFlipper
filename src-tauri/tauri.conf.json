{
	"build": {
		"beforeBuildCommand": "pnpm run build",
		"beforeDevCommand": "pnpm run dev",
		"devPath": "http://localhost:5173",
		"distDir": "../build"
	},
	"package": {
		"productName": "SkyblockFlipper",
		"version": "0.1.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"fs": {
				"all": false
			},
			"window": {
				"all": true
			},
			"protocol": {
				"all": false,
				"asset": true,
				"assetScope": ["**"]
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "com.sw1ndler.dev",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": "default-src 'self'; img-src 'self' asset: https://asset.localhost; media-src 'self' asset: https://asset.localhost"
		},

		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"decorations": false,
				"resizable": true,
				"height": 600,
				"width": 800,
				"title": "ah-flipper"
			}
		]
	}
}
